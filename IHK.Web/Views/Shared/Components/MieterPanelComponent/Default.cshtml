
<div class="row">
    <div class="col-md-12">
        <div class="panel panel-primary">
            <div class="panel-heading">Mieter</div>
            <div class="panel-body">
                <div class="col-md-12 container-fluid">
                    <div class="row">
                        <th_enum th-label="Anrede" th-multiple="false" th-value="mieter.anrede" th-type="@typeof(Anrede)" th-col="2"></th_enum>
                        <th_li th-label="Name" th-input="mieter.name" th-col="3"></th_li>
                        <th_li th-label="Vorname" th-input="mieter.vorname" th-col="3"></th_li>
                        <th_li th-label="Telefon" th-input="mieter.telefon" th-col="2"></th_li>
                        <th_li th-label="WBSNummer" th-input="mieter.wbsNummer" th-col="2"></th_li>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>